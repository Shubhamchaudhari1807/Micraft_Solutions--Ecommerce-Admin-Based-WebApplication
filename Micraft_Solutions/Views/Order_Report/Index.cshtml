@model IEnumerable<Micraft_Solutions.Data_Context.Sp_m_t_order_Result_Result>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="card">
     /.card-header 
    <div class="card-body">
        <table id="example1" class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>Date</th>
                    <th>Customer Name</th>
                    <th>Address</th>
                    <th>Contact </th>
                    <th>Product</th>
                    <th>Qty</th>
                    <th>Rate </th>
                    <th>Amount</th>
                  
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.order_date)
                        @Html.HiddenFor(modelItem => item.Order_id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.customer_name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.customer_address)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.customer_contact)
                    </td>
                    @*<td>
                        @Html.DisplayFor(modelItem => item.attr1)
                    </td>*@
                    <td>
                        @Html.DisplayFor(modelItem => item.order_qty)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.order_rate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.order_amount)
                    </td>

                  





                </tr>
                }
            </tbody>
            <tfoot>
                <tr>
                    <th>Date</th>
                    <th>Customer Name</th>
                    <th>Address</th>
                    <th>Contact </th>
                    <th>Product</th>
                    <th>Qty</th>
                    <th>Rate </th>
                    <th>Amount</th>

                   

                </tr>
            </tfoot>
        </table>
         <input type="button" class="btn btn-dark" value="Genrate Excel File" onclick="location.href='@Url.Action("GenrateExcelFile", "Student_Enquire")'" />
    </div>
</div>