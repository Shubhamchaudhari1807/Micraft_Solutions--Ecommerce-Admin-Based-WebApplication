@model Micraft_Solutions.Models.Contact_Us
<head>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    @if (TempData["message"] != null)
    {
        <script type="text/javascript">
						window.onload = function () {
							var message = '@TempData["message"]';
					 swal("Registration!", message, "success");
						};
        </script>
    }
    @if (TempData["error"] != null)
    {
        <script type="text/javascript">
						window.onload = function () {
							var errormsg= '@TempData["error"]';
					 swal("Oops Somthing Wrong!", errormsg, "error");
						};
        </script>
    }
    <!-- basic -->
    <meta charset="utf-8">
    <a href="~/Views/Feedback/Feedback_Partial_View.cshtml"></a>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- mobile metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1">
    <!-- site metas -->
    <title>rento</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- bootstrap css -->
    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css">
    <!-- style css -->
    <link rel="stylesheet" href="~/Content/css/style.css">
    <!-- Responsive-->
    <link rel="stylesheet" href="~/Content/css/responsive.css">
    <!-- fevicon -->
    <link rel="icon" href="~/Content/images/fevicon.png" type="image/gif" />
    <!-- Scrollbar Custom CSS -->
    <link rel="stylesheet" href="~/Content/css/jquery.mCustomScrollbar.min.css">
    <!-- Tweaks for older IEs-->
    <link rel="stylesheet" href="https://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.5/jquery.fancybox.min.css" media="screen">
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script><![endif]-->
    <script type="text/javascript">
    function Travale_Amount() {
            var get_km = $("#id_km").val();
            $("#id_amount").val(get_km * 13);
        }</script>
</head>
<div class="container-fluid">
    @using (Html.BeginForm("Save", "Contact_Us", FormMethod.Post))
    {
        <center>
            <h3>Email us with any questions or inquiries or call.</h3>
            <h3>We would be happy to answer your questions and set up a meeting with you.</h3>
            <h3>OUR ONLINE SHOP HELP YOU IN BEST WAY</h3>
            <h2>Mobile Number= 9112467147</h2>
            <h2>Email Id= Ecom.Micraft@gmail.com</h2>
        </center>
        <br>
        <strike></strike>
        <br>
        <br>

        <div class="row">
            <div class="col-md-3">
                <div class="form-group">
                    <label>Full Name<span style="color:red">*</span></label>
                    @Html.HiddenFor(m => m.cust_id)
                    @Html.TextBoxFor(m => m.Full_Name, new { @class = "form-control form-control-sm sm-2 border border-secondary", autofocus = "autofocus" })
                    @Html.ValidationMessageFor(m => m.Full_Name, "", new { @class = "text-danger" })

                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label>Email <span style="color:red">*</span></label>
                    @Html.TextBoxFor(m => m.Email, new { @class = "form-control form-control-sm sm-2 border border-secondary", autofocus = "autofocus" })
                    @Html.ValidationMessageFor(m => m.Email, "", new { @class = "text-danger" })

                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label>Your Message <span style="color:red">*</span></label>
                    @Html.TextAreaFor(m => m.Customer_Message, new { @class = "form-control form-control-sm sm-2 border border-secondary", autofocus = "autofocus" })
                    @Html.ValidationMessageFor(m => m.Customer_Message, "", new { @class = "text-danger" })

                </div>
            </div>

            <div class="col-md-3">
                <div class="form-group">
                    <label>Are You Existing Customer </label>
                    @Html.DropDownListFor(m => m.attribute1, new SelectList(ViewBag._ex_Cust, "Value", "Text"), new { @class = "form-control form-control-sm sm-2 border border-secondary", autofocus = "autofocus" })
                </div>
            </div>
        </div>
        <div class="col-md-3">
            <div class="form-group">
                <button type="submit" class="btn btn-dark">Submit</button>
            </div>
        </div>
        <div class="col-md-3">
            <div class="form-group">
                <a class="nav-link" href="/home/index">Go To Home</a>
            </div>
        </div>


    }
</div>
<script src="~/Content/js/jquery.min.js"></script>
<script src="~/Content/js/popper.min.js"></script>
<script src="~/Content/js/bootstrap.bundle.min.js"></script>
<script src="~/Content/js/jquery-3.0.0.min.js"></script>
<script src="~/Content/js/plugin.js"></script>
<!-- sidebar -->
<script src="~/Content/js/jquery.mCustomScrollbar.concat.min.js"></script>
<script src="~/Content/js/custom.js"></script>
<script src="https:cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.5/jquery.fancybox.min.js"></script>
